<!DOCTYPE html>
<html>
<head>
  <title>Dinefy</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="nav">
  <% if signed_in? %>
  <%= link_to "Log Out", session_path('current'), method: :delete %>
  <% else %>
    <%= link_to "Login", new_session_path %> | <%= link_to "Sign Up", new_user_path %>
    <% end %>
</div>

<%= yield %>

</body>
</html>
